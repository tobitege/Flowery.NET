    <!-- ==================== WIN95 DELETE ==================== -->
    <!-- Win95Delete - Papers flying INTO recycle bin from above -->
    <Style Selector="controls|DaisyLoading[Variant=Win95Delete]">
        <Setter Property="Template">
            <ControlTemplate>
                <Viewbox Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Stretch="Uniform">
                    <Canvas Width="96" Height="96" ClipToBounds="True">
                        <!-- Large centered recycle bin -->
                        <!-- Lid handle -->
                        <Border Width="12" Height="4" Canvas.Left="42" Canvas.Top="28"
                                Background="{TemplateBinding Foreground}" CornerRadius="2"/>
                        <!-- Lid -->
                        <Border Width="52" Height="10" Canvas.Left="22" Canvas.Top="32"
                                BorderBrush="{TemplateBinding Foreground}" BorderThickness="2"
                                CornerRadius="3,3,0,0" Background="Transparent"/>
                        <!-- Bin body -->
                        <Border Width="46" Height="44" Canvas.Left="25" Canvas.Top="42"
                                BorderBrush="{TemplateBinding Foreground}" BorderThickness="2"
                                CornerRadius="0,0,6,6" Background="Transparent">
                            <StackPanel Orientation="Horizontal" Spacing="6"
                                        HorizontalAlignment="Center" VerticalAlignment="Center">
                                <Border Width="2" Height="28" Background="{TemplateBinding Foreground}" Opacity="0.3"/>
                                <Border Width="2" Height="28" Background="{TemplateBinding Foreground}" Opacity="0.3"/>
                                <Border Width="2" Height="28" Background="{TemplateBinding Foreground}" Opacity="0.3"/>
                            </StackPanel>
                        </Border>
                        <!-- Flying papers - start above bin, fly down into lid -->
                        <Border Classes="del-paper1" Width="10" Height="12" Canvas.Left="26" Canvas.Top="4"
                                BorderBrush="{TemplateBinding Foreground}" BorderThickness="1.2"
                                CornerRadius="1" Background="Transparent"
                                RenderTransformOrigin="0.5,0.5">
                            <StackPanel Spacing="1.5" Margin="1.5,2,1.5,1.5">
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                            </StackPanel>
                        </Border>
                        <Border Classes="del-paper2" Width="10" Height="12" Canvas.Left="43" Canvas.Top="2"
                                BorderBrush="{TemplateBinding Foreground}" BorderThickness="1.2"
                                CornerRadius="1" Background="Transparent"
                                RenderTransformOrigin="0.5,0.5">
                            <StackPanel Spacing="1.5" Margin="1.5,2,1.5,1.5">
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                            </StackPanel>
                        </Border>
                        <Border Classes="del-paper3" Width="10" Height="12" Canvas.Left="60" Canvas.Top="4"
                                BorderBrush="{TemplateBinding Foreground}" BorderThickness="1.2"
                                CornerRadius="1" Background="Transparent"
                                RenderTransformOrigin="0.5,0.5">
                            <StackPanel Spacing="1.5" Margin="1.5,2,1.5,1.5">
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                            </StackPanel>
                        </Border>
                    </Canvas>
                </Viewbox>
            </ControlTemplate>
        </Setter>
    </Style>

