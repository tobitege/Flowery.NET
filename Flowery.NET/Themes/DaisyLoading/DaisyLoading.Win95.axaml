    <!-- ==================== WIN95 SEARCH ==================== -->
    <!-- Win95Search - Flashlight sweeping over folders -->
    <Style Selector="controls|DaisyLoading[Variant=Win95Search]">
        <Setter Property="Template">
            <ControlTemplate>
                <Viewbox Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Stretch="Uniform">
                    <Grid Width="96" Height="96">
                        <!-- Row of folders at bottom -->
                        <StackPanel Orientation="Horizontal" Spacing="4"
                                    HorizontalAlignment="Center" VerticalAlignment="Bottom"
                                    Margin="0,0,0,16">
                            <!-- Folder 1 -->
                            <Canvas Width="20" Height="20" Opacity="0.4">
                                <Border Width="10" Height="4" Canvas.Left="0" Canvas.Top="0"
                                        BorderBrush="{TemplateBinding Foreground}" BorderThickness="1.6,1.6,1.6,0"
                                        CornerRadius="2,2,0,0" Background="Transparent"/>
                                <Border Width="20" Height="16" Canvas.Left="0" Canvas.Top="4"
                                        BorderBrush="{TemplateBinding Foreground}" BorderThickness="1.6"
                                        CornerRadius="0,2,2,2" Background="Transparent"/>
                            </Canvas>
                            <!-- Folder 2 -->
                            <Canvas Width="20" Height="20" Opacity="0.4">
                                <Border Width="10" Height="4" Canvas.Left="0" Canvas.Top="0"
                                        BorderBrush="{TemplateBinding Foreground}" BorderThickness="1.6,1.6,1.6,0"
                                        CornerRadius="2,2,0,0" Background="Transparent"/>
                                <Border Width="20" Height="16" Canvas.Left="0" Canvas.Top="4"
                                        BorderBrush="{TemplateBinding Foreground}" BorderThickness="1.6"
                                        CornerRadius="0,2,2,2" Background="Transparent"/>
                            </Canvas>
                            <!-- Folder 3 -->
                            <Canvas Width="20" Height="20" Opacity="0.4">
                                <Border Width="10" Height="4" Canvas.Left="0" Canvas.Top="0"
                                        BorderBrush="{TemplateBinding Foreground}" BorderThickness="1.6,1.6,1.6,0"
                                        CornerRadius="2,2,0,0" Background="Transparent"/>
                                <Border Width="20" Height="16" Canvas.Left="0" Canvas.Top="4"
                                        BorderBrush="{TemplateBinding Foreground}" BorderThickness="1.6"
                                        CornerRadius="0,2,2,2" Background="Transparent"/>
                            </Canvas>
                            <!-- Folder 4 -->
                            <Canvas Width="20" Height="20" Opacity="0.4">
                                <Border Width="10" Height="4" Canvas.Left="0" Canvas.Top="0"
                                        BorderBrush="{TemplateBinding Foreground}" BorderThickness="1.6,1.6,1.6,0"
                                        CornerRadius="2,2,0,0" Background="Transparent"/>
                                <Border Width="20" Height="16" Canvas.Left="0" Canvas.Top="4"
                                        BorderBrush="{TemplateBinding Foreground}" BorderThickness="1.6"
                                        CornerRadius="0,2,2,2" Background="Transparent"/>
                            </Canvas>
                        </StackPanel>
                        <!-- Flashlight + beam (sweeps left to right) -->
                        <Canvas Classes="flashlight-group" Width="96" Height="96">
                            <!-- Light beam (trapezoid approximation) -->
                            <Path Fill="{TemplateBinding Foreground}" Opacity="0.15"
                                  Data="M44,38 L52,38 L62,58 L34,58 Z"/>
                            <!-- Flashlight handle -->
                            <Border Width="14" Height="7" Canvas.Left="30" Canvas.Top="28"
                                    BorderBrush="{TemplateBinding Foreground}" BorderThickness="2"
                                    CornerRadius="2" Background="Transparent"/>
                            <!-- Flashlight head -->
                            <Border Width="16" Height="10" Canvas.Left="44" Canvas.Top="26"
                                    BorderBrush="{TemplateBinding Foreground}" BorderThickness="2"
                                    CornerRadius="0,3,3,0" Background="Transparent"/>
                            <!-- Lens -->
                            <Ellipse Width="5" Height="5" Canvas.Left="57" Canvas.Top="28.5"
                                     Fill="{TemplateBinding Foreground}" Opacity="0.6"/>
                        </Canvas>
                    </Grid>
                </Viewbox>
            </ControlTemplate>
        </Setter>
    </Style>

    <Style Selector="controls|DaisyLoading[Variant=Win95Search] /template/ Canvas.flashlight-group">
        <Style.Animations>
            <Animation Duration="0:0:2" IterationCount="Infinite">
                <KeyFrame KeyTime="0:0:0"><Setter Property="TranslateTransform.X" Value="-24"/></KeyFrame>
                <KeyFrame KeyTime="0:0:1"><Setter Property="TranslateTransform.X" Value="24"/></KeyFrame>
                <KeyFrame KeyTime="0:0:2"><Setter Property="TranslateTransform.X" Value="-24"/></KeyFrame>
            </Animation>
        </Style.Animations>
    </Style>

    <!-- ==================== WIN95 EMPTY RECYCLE ==================== -->
    <!-- Win95EmptyRecycle - Papers flying OUT of recycle bin upward -->
    <Style Selector="controls|DaisyLoading[Variant=Win95EmptyRecycle]">
        <Setter Property="Template">
            <ControlTemplate>
                <Viewbox Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Stretch="Uniform">
                    <Canvas Width="96" Height="96" ClipToBounds="True">
                        <!-- Large centered recycle bin -->
                        <!-- Lid handle -->
                        <Border Width="12" Height="4" Canvas.Left="42" Canvas.Top="28"
                                Background="{TemplateBinding Foreground}" CornerRadius="2"/>
                        <!-- Lid -->
                        <Border Width="52" Height="10" Canvas.Left="22" Canvas.Top="32"
                                BorderBrush="{TemplateBinding Foreground}" BorderThickness="2"
                                CornerRadius="3,3,0,0" Background="Transparent"/>
                        <!-- Bin body -->
                        <Border Width="46" Height="44" Canvas.Left="25" Canvas.Top="42"
                                BorderBrush="{TemplateBinding Foreground}" BorderThickness="2"
                                CornerRadius="0,0,6,6" Background="Transparent">
                            <StackPanel Orientation="Horizontal" Spacing="6"
                                        HorizontalAlignment="Center" VerticalAlignment="Center">
                                <Border Width="2" Height="28" Background="{TemplateBinding Foreground}" Opacity="0.3"/>
                                <Border Width="2" Height="28" Background="{TemplateBinding Foreground}" Opacity="0.3"/>
                                <Border Width="2" Height="28" Background="{TemplateBinding Foreground}" Opacity="0.3"/>
                            </StackPanel>
                        </Border>
                        <!-- Papers all start at center of lid, fly outward -->
                        <Border Classes="empty-paper1" Width="10" Height="12" Canvas.Left="43" Canvas.Top="34"
                                BorderBrush="{TemplateBinding Foreground}" BorderThickness="1.2"
                                CornerRadius="1" Background="Transparent"
                                RenderTransformOrigin="0.5,0.5">
                            <StackPanel Spacing="1.5" Margin="1.5,2,1.5,1.5">
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                            </StackPanel>
                        </Border>
                        <Border Classes="empty-paper2" Width="10" Height="12" Canvas.Left="43" Canvas.Top="34"
                                BorderBrush="{TemplateBinding Foreground}" BorderThickness="1.2"
                                CornerRadius="1" Background="Transparent"
                                RenderTransformOrigin="0.5,0.5">
                            <StackPanel Spacing="1.5" Margin="1.5,2,1.5,1.5">
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                            </StackPanel>
                        </Border>
                        <Border Classes="empty-paper3" Width="10" Height="12" Canvas.Left="43" Canvas.Top="34"
                                BorderBrush="{TemplateBinding Foreground}" BorderThickness="1.2"
                                CornerRadius="1" Background="Transparent"
                                RenderTransformOrigin="0.5,0.5">
                            <StackPanel Spacing="1.5" Margin="1.5,2,1.5,1.5">
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                            </StackPanel>
                        </Border>
                    </Canvas>
                </Viewbox>
            </ControlTemplate>
        </Setter>
    </Style>

    <!-- Papers fade in at center, fly UP, fade out at top -->
    <Style Selector="controls|DaisyLoading[Variant=Win95EmptyRecycle] /template/ Border.empty-paper1">
        <Style.Animations>
            <Animation Duration="0:0:1.6" IterationCount="Infinite">
                <KeyFrame KeyTime="0:0:0">
                    <Setter Property="TranslateTransform.X" Value="0"/>
                    <Setter Property="TranslateTransform.Y" Value="0"/>
                    <Setter Property="Opacity" Value="0"/>
                </KeyFrame>
                <KeyFrame KeyTime="0:0:0.05">
                    <Setter Property="TranslateTransform.X" Value="0"/>
                    <Setter Property="TranslateTransform.Y" Value="0"/>
                    <Setter Property="Opacity" Value="1"/>
                </KeyFrame>
                <KeyFrame KeyTime="0:0:0.5">
                    <Setter Property="TranslateTransform.X" Value="-12"/>
                    <Setter Property="TranslateTransform.Y" Value="-28"/>
                    <Setter Property="Opacity" Value="1"/>
                </KeyFrame>
                <KeyFrame KeyTime="0:0:0.6">
                    <Setter Property="TranslateTransform.X" Value="-14"/>
                    <Setter Property="TranslateTransform.Y" Value="-32"/>
                    <Setter Property="Opacity" Value="0"/>
                </KeyFrame>
                <KeyFrame KeyTime="0:0:1.6">
                    <Setter Property="TranslateTransform.X" Value="-14"/>
