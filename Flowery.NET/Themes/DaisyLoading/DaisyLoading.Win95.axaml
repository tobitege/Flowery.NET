<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controls="clr-namespace:Flowery.Controls;assembly=Flowery.NET">

    <!-- ==================== WIN95 FILE COPY ==================== -->
    <!-- Win95FileCopy - Folders with flying papers in arc -->
    <Style Selector="controls|DaisyLoading[Variant=Win95FileCopy]">
        <Setter Property="Template">
            <ControlTemplate>
                <Viewbox Width="{TemplateBinding Width}" Height="{TemplateBinding Height}" Stretch="Uniform">
                    <Grid Width="96" Height="96">
                        <!-- Left folder -->
                        <Canvas HorizontalAlignment="Left" VerticalAlignment="Center" Margin="12,10,0,0">
                            <!-- Folder tab -->
                            <Border Width="14" Height="6" Canvas.Left="0" Canvas.Top="0"
                                    BorderBrush="{TemplateBinding Foreground}" BorderThickness="2,2,2,0"
                                    CornerRadius="2,2,0,0" Background="Transparent"/>
                            <!-- Folder body -->
                            <Border Width="32" Height="20" Canvas.Left="0" Canvas.Top="6"
                                    BorderBrush="{TemplateBinding Foreground}" BorderThickness="2"
                                    CornerRadius="0,2,2,2" Background="Transparent"/>
                        </Canvas>
                        <!-- Right folder -->
                        <Canvas HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0,10,12,0">
                            <!-- Folder tab -->
                            <Border Width="14" Height="6" Canvas.Left="0" Canvas.Top="0"
                                    BorderBrush="{TemplateBinding Foreground}" BorderThickness="2,2,2,0"
                                    CornerRadius="2,2,0,0" Background="Transparent"/>
                            <!-- Folder body -->
                            <Border Width="32" Height="20" Canvas.Left="0" Canvas.Top="6"
                                    BorderBrush="{TemplateBinding Foreground}" BorderThickness="2"
                                    CornerRadius="0,2,2,2" Background="Transparent"/>
                        </Canvas>
                        <!-- Flying papers -->
                        <Border Classes="paper1" Width="10" Height="12" Margin="28,32,0,0"
                                BorderBrush="{TemplateBinding Foreground}" BorderThickness="1.2"
                                CornerRadius="1" Background="Transparent" Opacity="0"
                                HorizontalAlignment="Left" VerticalAlignment="Top">
                            <StackPanel Spacing="1.5" Margin="1.5,2,1.5,1.5">
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                            </StackPanel>
                        </Border>
                        <Border Classes="paper2" Width="10" Height="12" Margin="28,30,0,0"
                                BorderBrush="{TemplateBinding Foreground}" BorderThickness="1.2"
                                CornerRadius="1" Background="Transparent" Opacity="0"
                                HorizontalAlignment="Left" VerticalAlignment="Top">
                            <StackPanel Spacing="1.5" Margin="1.5,2,1.5,1.5">
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                            </StackPanel>
                        </Border>
                        <Border Classes="paper3" Width="10" Height="12" Margin="28,28,0,0"
                                BorderBrush="{TemplateBinding Foreground}" BorderThickness="1.2"
                                CornerRadius="1" Background="Transparent" Opacity="0"
                                HorizontalAlignment="Left" VerticalAlignment="Top">
                            <StackPanel Spacing="1.5" Margin="1.5,2,1.5,1.5">
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                                <Border Height="1" Background="{TemplateBinding Foreground}" Opacity="0.4"/>
                            </StackPanel>
                        </Border>
                    </Grid>
                </Viewbox>
            </ControlTemplate>
        </Setter>
    </Style>

    <!-- Paper flying animations with arc motion -->
    <Style Selector="controls|DaisyLoading[Variant=Win95FileCopy] /template/ Border.paper1">
        <Style.Animations>
            <Animation Duration="0:0:1.6" IterationCount="Infinite">
                <KeyFrame KeyTime="0:0:0">
                    <Setter Property="TranslateTransform.X" Value="0"/>
                    <Setter Property="TranslateTransform.Y" Value="0"/>
                    <Setter Property="Opacity" Value="1"/>
                </KeyFrame>
                <KeyFrame KeyTime="0:0:0.4">
                    <Setter Property="TranslateTransform.X" Value="20"/>
                    <Setter Property="TranslateTransform.Y" Value="-8"/>
                    <Setter Property="Opacity" Value="1"/>
                </KeyFrame>
                <KeyFrame KeyTime="0:0:0.8">
                    <Setter Property="TranslateTransform.X" Value="42"/>
                    <Setter Property="TranslateTransform.Y" Value="0"/>
                    <Setter Property="Opacity" Value="1"/>
                </KeyFrame>
                <KeyFrame KeyTime="0:0:0.81">
                    <Setter Property="TranslateTransform.X" Value="42"/>
                    <Setter Property="TranslateTransform.Y" Value="0"/>
                    <Setter Property="Opacity" Value="0"/>
                </KeyFrame>
                <KeyFrame KeyTime="0:0:1.59">
                    <Setter Property="TranslateTransform.X" Value="0"/>
                    <Setter Property="TranslateTransform.Y" Value="0"/>
                    <Setter Property="Opacity" Value="0"/>
                </KeyFrame>
                <KeyFrame KeyTime="0:0:1.6">
                    <Setter Property="TranslateTransform.X" Value="0"/>
                    <Setter Property="TranslateTransform.Y" Value="0"/>
                    <Setter Property="Opacity" Value="1"/>
                </KeyFrame>
            </Animation>
        </Style.Animations>
    </Style>

