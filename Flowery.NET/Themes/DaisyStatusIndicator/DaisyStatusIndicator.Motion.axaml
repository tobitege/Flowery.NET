    <!-- First orbiting dot -->
    <Style Selector="controls|DaisyStatusIndicator[Variant=Spin][Size=Medium] /template/ Ellipse#PART_AnimationEllipse">
        <Setter Property="IsVisible" Value="True" />
        <Setter Property="Opacity" Value="0.9" />
        <Setter Property="Width" Value="3" />
        <Setter Property="Height" Value="3" />
        <Setter Property="RenderTransformOrigin" Value="50%,50%" />
        <Style.Animations>
            <Animation Duration="0:0:1" IterationCount="Infinite">
                <KeyFrame KeyTime="0:0:0">
                    <Setter Property="TranslateTransform.X" Value="7" />
                    <Setter Property="TranslateTransform.Y" Value="0" />
                </KeyFrame>
                <KeyFrame KeyTime="0:0:0.125">
                    <Setter Property="TranslateTransform.X" Value="4.95" />
                    <Setter Property="TranslateTransform.Y" Value="4.95" />
                </KeyFrame>
                <KeyFrame KeyTime="0:0:0.25">
                    <Setter Property="TranslateTransform.X" Value="0" />
                    <Setter Property="TranslateTransform.Y" Value="7" />
                </KeyFrame>
                <KeyFrame KeyTime="0:0:0.375">
                    <Setter Property="TranslateTransform.X" Value="-4.95" />
                    <Setter Property="TranslateTransform.Y" Value="4.95" />
                </KeyFrame>
                <KeyFrame KeyTime="0:0:0.5">
                    <Setter Property="TranslateTransform.X" Value="-7" />
                    <Setter Property="TranslateTransform.Y" Value="0" />
                </KeyFrame>
                <KeyFrame KeyTime="0:0:0.625">
                    <Setter Property="TranslateTransform.X" Value="-4.95" />
                    <Setter Property="TranslateTransform.Y" Value="-4.95" />
                </KeyFrame>
                <KeyFrame KeyTime="0:0:0.75">
                    <Setter Property="TranslateTransform.X" Value="0" />
                    <Setter Property="TranslateTransform.Y" Value="-7" />
                </KeyFrame>
                <KeyFrame KeyTime="0:0:0.875">
                    <Setter Property="TranslateTransform.X" Value="4.95" />
                    <Setter Property="TranslateTransform.Y" Value="-4.95" />
                </KeyFrame>
                <KeyFrame KeyTime="0:0:1">
                    <Setter Property="TranslateTransform.X" Value="7" />
                    <Setter Property="TranslateTransform.Y" Value="0" />
                </KeyFrame>
            </Animation>
        </Style.Animations>
    </Style>
